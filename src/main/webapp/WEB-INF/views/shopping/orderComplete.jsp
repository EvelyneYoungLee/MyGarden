<script type="text/javascript">
  $(document).ready(function() {
    // 바로 주문하기
    $(".direct_order").click(function() {
      if( $("input[name=custom_length]").val() > 1 ) {
        swal("","장바구니에 2개 이상의 정기배송박스를 담으셨네요 !\n한 주문 당 하나의 박스만 결제가 가능합니다.","warning");
        return;
      } else {
        go_settle();
      }
    });
    // 장바구니 수정하기
    $(".direct_cart").click(function() {
      document.location.href = "/order/cart";
    });
  });

  function go_settle() {
    var box_seq = $("input[name=cart_box_seq]").val();

    if( typeof box_seq == "undefined" || box_seq == "" ) {
      box_seq = "";
    }
    
    $.ajax({
      "url"  : "/ver4/go_settle",
      "type" : "post",
      "data" : "box_seq="+box_seq,
      "success" : function(result){
        document.location.href = "/order/settle";
      }
    });
     
  }

  function show_cart_layer() {
    $("#cart_layer").toggle();
  }

  function get_cart_good() {
    $.ajax({
      "url" : "/ver4/get_cart_goods",
      "success" : function(obj){
        var getObj = jQuery.parseJSON( obj );
        var obj_normalcart = getObj.normal;
        var obj_customcart = getObj.custom;

        var cartHtml = "";
        var customHtml = "";
        var customTitleHtml = "";
        var cartTitleHtml = "";


        // 장바구니에 담긴 상품이 없을때
        if( getObj == "empty" || getObj == "" ) {
          cartHtml += "<div class='cart_empty'>장바구니에 담은 상품이 없습니다.</div>";

          $("#cart_body").css({"width":"100%","border-right":"none","margin-right":"0px"});
        } 
        else {

          // 일반 장바구니 타이틀 html
          cartTitleHtml += "<div class='normalcarttitle' style='height:22px; margin-bottom:10px;'>";
          cartTitleHtml += " <div style='display:inline-block; background-color:#559c75; padding:3px 7px; color:#ffffff; font-size:10px; border-radius:15px; letter-spacing:0px; font-weight:normal;'>일반구매</div>";
          cartTitleHtml += "</div>";

          // 일반 장바구니에 담긴 내역
          for( var i=0; i<obj_normalcart.length; i++ ) {
            cartHtml += "<div class='cart_layer_goods'>";
            cartHtml += " <div><a href='/manna/view?no="+obj_normalcart[i].goods_seq+"'><img src='"+obj_normalcart[i].goodsImg+"' width='120' /></a></div>";
            cartHtml += " <div class='cart_goods_name'>"+obj_normalcart[i].goods_name+"</div>";
            cartHtml += " <div class='cart_goods_option1'>"+obj_normalcart[i].option1+"</div>";
            cartHtml += " <span>총 </span><div class='cart_goods_price'>"+obj_normalcart[i].tot_price+"</div><span> 원 / </span>";
            cartHtml += " <div class='cart_goods_ea'>"+obj_normalcart[i].ea+"개</div>";
            cartHtml += " <div class='cart_goods_date'>"+obj_normalcart[i].cart_inputs[0]['input_value']+"</div>";
            cartHtml += "</div>";
          }

          // 정기배송박스에 담은 내역이 없을때 일반 장바구니 영역 100%
          if( obj_customcart == "" ) {
            $("#cart_body").css({"width":"100%","border-right":"none","margin-right":"0px"});
          }

          // 일반 장바구니에에 담은 내역이 없을때 정기배송장바구니 영역 100%
          if( obj_normalcart == "" ) {
            $("#cart_body_custom").css({"width":"100%"});
            cartTitleHtml = "";
          }

          // 이전 box_seq 
          var preSeq = "";
          // 정기배송박스에 담긴 내역
          for( var i=0; i<obj_customcart.length; i++ ) { 

            if( obj_customcart[i].box_seq != preSeq ) {
              // 정기배송박스에 담긴 내역이 있을때 박스 이름과 정기배송 뱃지 html
              if( preSeq == "" ) {
                 customHtml += "<div style='height:22px; margin-bottom:10px;'>";
               } else {
                 customHtml += "<div style='height:22px; margin-top: 15px; margin-bottom:10px;'>";
               }
             
              customHtml += " <div style='display:inline-block; background-color:#00a8ff; padding:3px 7px; color:#ffffff; font-size:10px; border-radius:15px; letter-spacing:0px; font-weight:normal;'>정기배송</div>";
              customHtml += " <div style='display:inline-block; font-size: 13px; letter-spacing: -0.5px; color: #515151;'>"+obj_customcart[i].box_name+"</div>";
              customHtml += " <input type='hidden' name='cart_box_seq' value='"+obj_customcart[i].box_seq+"' />";
              customHtml += "</div>";
            } 
            preSeq = obj_customcart[i].box_seq;

            // 정기배송박스에 담은 상품의 배송주기
            var customWeekHtml = "";
            if(obj_customcart[i].shipping_term == 'custom_1week') {
              customWeekHtml = "<div class='shipping_1week'>매주</div>";
            } else if(obj_customcart[i].shipping_term == 'custom_2week') {
              customWeekHtml = "<div class='shipping_2week'>격주</div>";
            }

            // 정기배송박스에 담긴 상품의 총 합
            var customTotPrice = "";
            customTotPrice = obj_customcart[i].price * obj_customcart[i].ea;
            
            customHtml += "<div class='cart_layer_goods'>";
            customHtml += " <div><a href='/manna/view?no="+obj_customcart[i].goods_seq+"'><img src='"+obj_customcart[i].goodsImg+"' width='120' /></a></div>";
            customHtml += " <div class='cart_goods_name'>"+customWeekHtml+obj_customcart[i].goods_name+"</div>";
            customHtml += " <div class='cart_goods_option1'>"+obj_customcart[i].goods_option+"</div>";
            customHtml += " <span>총 </span><div class='cart_goods_price'>"+customTotPrice+"</div><span> 원 / </span>";
            customHtml += " <div class='cart_goods_ea'>"+obj_customcart[i].ea+"개</div>";
            customHtml += " <div class='cart_goods_date'>"+obj_customcart[i].want_date+" 도착</div>";
            customHtml += "</div>";

          }
        }

        $("#cart_body_custom").html( customHtml );
        $("#cart_body").html( cartHtml );
        $(".cart_goods_price").number(true);

        // 일반 카트 타이틀
        $("#cart_body").prepend(cartTitleHtml);

        if( $("input[name=cart_box_seq]").length != 0 ) {
          $("input[name=custom_length]").val( $("input[name=cart_box_seq]").length );
        }

        show_cart_layer();
        get_cartCount();
      }
   });
  }
</script>
