<script type="text/javascript">
  $(document).ready(function() {
    // 장바구니에 담겨있는 상품의 수
    get_cartCount();

    // 장바구니 페이지 이동하기
      $("#cart_add_layer").click(function(){ document.location.href="/order/cart"; });

    var top_line_banner_ck = $.cookie('top_line_banner');
    if( top_line_banner_ck != "hide" ){
      $('#top_line_banner').show();
    }
  });

  // 카트에 담겨져 있는 상품의 수
  function get_cartCount() {
     $.ajax({
        "url" : "/order/get_cartCount",
        "success" : function(result){
           if( result == 'empty' || result == '' ) {
              $(".inCart_ea_wrap").hide();
           } else {
              $(".inCart_ea").html(result);
              $(".inCart_ea_wrap").show();
           }
        }
     });
  }

  function visible_real_cat() {
      if( $(".real_cat").css("display") != "none" ) {
        $(document).mouseup(function (e) { 
          if(!$(".real_cat").is(e.target)&& $(".real_cat").has(e.target).length === 0) { 
            $(".real_cat").slideUp(); 
          } 
        });
      }
    }

  // 하위메뉴
  function show_dropMenu() {
    $(".real_cat").slideDown();
  }

  var mouse_is_inside = false;
    $(document).ready(function() {
      // 헤드 다운메뉴
      $(".down_menu").hover(function(){ 
          mouse_is_inside=true; 
      },
       function(){ 
          mouse_is_inside=false; 
      });

      // 헤드 햄버거 메뉴
      $(".real_cat").hover(function(){ 
          mouse_is_inside=true; 
      },
       function(){ 
          mouse_is_inside=false; 
      });

      $(document).mouseup(function(){ 
        if(! mouse_is_inside) {
          $(".real_cat").slideUp();
          $(".down_menu").hide();
          $(".up_manna").hide(); 
          $(".up_event").hide(); 
          $(".down_manna").show();
          $(".down_event").show();
        }
      });
    });

  // 띠배너 숨기기
  function hide_top_line_banner() {
    // $('#top_line_banner').hide();
    $('#top_line_banner').fadeOut();
    $(".top_banner_height").css("margin-top", "185px");

    var date = new Date();
    date.setTime(date.getTime() + 3*60*1000);

    ga('send', 'event', 'pc_banner_close', 'click', '');

    $.cookie("top_line_banner", "hide", { expires:date });
  }

  function check_ga_banner() {
    var url = "/order_call";
    ga('send', 'event', 'pc_banner', 'click', url);

    document.location.href = url;
  }
</script>

